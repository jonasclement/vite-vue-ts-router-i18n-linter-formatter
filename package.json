{
  "name": "vite-vue-ts-router-i18n-linter-formatter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:scripts": "eslint --ext .js,ts,.vue --ignore-path .gitignore --fix src",
    "lint:styles": "stylelint --fix \"src/**/*.{scss,vue}\"",
    "lint:format": "prettier src --write",
    "lint": "npm run lint:scripts && npm run lint:styles && npm run lint:format",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "src/**/*.{scss,vue}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.32",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-eslint": "^1.3.0",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.33.1"
  }
}
